  <body>
  
  
    <div class="wrapper">

<% flash.keys.each do |alert_danger| %>
  <div class="alert_danger <%= alert_danger.to_s.gsub('_', '-') %> alert_danger-dismissible fade show" role="alert_danger">
    <%= flash[alert_danger] %>
    <button type="button" class="close" data-dismiss="alert_danger" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
 <% end %>

    <% if logged_in? %>
    <!-- Navbar (sit on top) -->
<div class="w3-top">
  <div class="w3-bar w3-white w3-padding w3-card" style="letter-spacing:4px;">
    <a href="/users/<%=current_user.slug%>"  class="w3-bar-item w3-button">My Profile</a>
    <a href="/users/<%=current_user.slug%>"  class="w3-bar-item w3-button">My Profile</a>
    <a href="/users/<%=current_user.slug%>"  class="w3-bar-item w3-button">My Profile</a>
    <a href="/users/<%=current_user.slug%>"  class="w3-bar-item w3-button">My Profile</a>
    <a href="/users/<%=current_user.slug%>"  class="w3-bar-item w3-button">My Profile</a>
    
  </div>
</div>
<div class="sidenav">
  <a href="/courses">My Courses</a>
  <a href="/courses/new">Add A New Course</a>
  <a href="/courses/assignments">Upcoming assignments</a>
  <a href="/courses/notes">My Notes</a>
  <h5><a href="/users/<%=current_user.slug%>">Account Settings</a>
  <h5><a href="/logout">Log Out</a>
</div>
   
      <% end %>

        <%= yield %>
      <footer class="branding">
        <small>&copy; 2020 <strong>EduTasks</strong></small>
      </footer>
    </div>

