  <body>
   
<div class="wrapper">
<% flash.keys.each do |alert_danger| %>
  <div class="alert_danger <%= alert_danger.to_s.gsub('_', '-') %> alert_danger-dismissible fade show" role="alert_danger">
    <%= flash[alert_danger] %>
    <button type="button" class="close" data-dismiss="alert_danger" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
 <% end %>

    <% if logged_in? %>
<header><div class="logo"><img src="/images/logo-cyan2.png" class="img-responsive main-image"></div></header>
<div class="header">

<div class="nav">
  <div><a href="/courses">My Courses</a></div>
  <div><a href="/courses/new">Add A New Course</a></div>
  <div><a href="/courses/assignments">Upcoming assignments</a></div>
  <div><a href="/courses/notes">My Notes</a></div>
  <div><a href="/users/<%=current_user.slug%>">Account Settings</a></div>
  <div><a href="/logout">Log Out</a></div>
</div>
</div>

   
      <% end %>
<div class="container">
        <%= yield %>
</div>
      <footer class="branding">
        <small>&copy; 2020 <strong>EduTasks</strong></small>
      </footer>
    </div>


