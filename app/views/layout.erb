          <a class="nav-link active" aria-current="page" href="/courses/assignments">Upcoming assignments</a>
          <a class="nav-link active" aria-current="page" href="/courses/notes">My Notes</a>
          <a class="nav-link active" aria-current="page" href="/users/<%=current_user.slug%>">Account Settings</a>
          <a class="nav-link active" aria-current="page" href="/logout">Log Out</a>
        </div>
        </nav>
