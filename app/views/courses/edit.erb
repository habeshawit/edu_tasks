<h1>Editing <%=@course.name%></h1>
<h2></h2>
<form method=post action="/courses/<%=@course.id%>">
    <input id="hidden" type="hidden" name="_method" value="patch">
  <label><strong> Schedule:</strong></label>
  <input type="text" name="schedule" value="<%= @course.schedule %>"><br>
  <label><strong> Assignments:</strong></label>
  <input type="text" name="assignments" value="<%= @course.assignments %>"><br>
  <label><strong> Notes:</strong></label>
  <input type="text" name="notes" value="<%= @course.notes %>"><br>
    <input type="submit" value="Save">

</form>
